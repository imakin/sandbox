{
    "name": "front-alpine",
    "version": "1.0.0",
    "description": "awas-modus",
    "private": true,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "compile": "sass src/style.scss src/style.min.css",
