        "http-server": "^14.1.1"
    },
    "devDependencies": {
        "gulp": "^4.0.2",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-buffer": "^1.1.1",
        "vinyl-source-stream": "^2.0.0"
    }
}
